        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/ArtisticMovement"/>
        <rdfs:label xml:lang="en">is programmatic definition of</rdfs:label>
        <skos:definition xml:lang="en">Manifesto of Futurism is the programmatic definition of Futurist.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/isRelatedTo -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/isRelatedTo">
