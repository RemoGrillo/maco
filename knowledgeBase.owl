        </rdfs:domain>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/ArtisticMovement"/>
        <rdfs:label xml:lang="en">guides</rdfs:label>
        <skos:definition xml:lang="en">Key Concept and Interpreted Concept guides Futurist.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasAbstractionIn -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasAbstractionIn">
