                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/M00"/>
        <rdfs:label xml:lang="en">is expressed in</rdfs:label>
        <skos:definition xml:lang="en">Key Concept or Interpreted is expressed in Manifesto.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/isGuidedBy -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/isGuidedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
