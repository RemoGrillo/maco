        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has equivalent concept</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has an equivalent concept in the compared Manifesto, embodied by the Interpreted Concept.</skos:definition>
    </owl:ObjectProperty>
    


