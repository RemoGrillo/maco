    <!-- https://remogrillo.github.io/maco/ArtisticMovement -->

    <owl:Class rdf:about="https://remogrillo.github.io/maco/ArtisticMovement">
        <rdfs:isDefinedBy xml:lang="en">http://vocab.getty.edu/page/aat/300021374</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Futurist</rdfs:label>
        <skos:definition xml:lang="en">Refers to the literary and artistic movement centered in Italy that emphasized speed, dynamism, energy and a rejection of the past, which began with the writings of Filippo Tommaso Marinetti in 1909. Futurism also developed in Russia, with more revolutionary and political connotations.</skos:definition>
    </owl:Class>
    


    <!-- https://remogrillo.github.io/maco/Attitude -->

