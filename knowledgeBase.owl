    <!-- https://remogrillo.github.io/maco/conveys -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/conveys">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://remogrillo.github.io/maco/isConveyedBy"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/M00"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/NewNorm"/>
        <rdfs:label xml:lang="en">conveys</rdfs:label>
        <skos:definition xml:lang="en">A Manifesto conveys a New Norm.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/expresses -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/expresses">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://remogrillo.github.io/maco/isExpressedIn"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/M00"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://remogrillo.github.io/maco/InterpretedConcept"/>
                    <rdf:Description rdf:about="https://remogrillo.github.io/maco/KeyConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">expresses</rdfs:label>
        <skos:definition xml:lang="en">A Manifesto expresses Key Concept or Interpreted Concept. If expresses one, cannot express the other.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/givesShapeTo -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/givesShapeTo">
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/M00"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/ArtisticMovement"/>
        <rdfs:label xml:lang="en">gives shape to</rdfs:label>
        <skos:definition xml:lang="en">What is Futurist as a whole, is determined and somehow regulated by the 22 Futurist Manifestos all together.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/guides -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/guides">
