        <skos:definition xml:lang="en">Compares any Futurist Manifesto to the Manifesto of Futurism.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/isConveyedBy -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/isConveyedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
