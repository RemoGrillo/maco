    <!-- https://remogrillo.github.io/maco/conveys -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/conveys">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://remogrillo.github.io/maco/isConveyedBy"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/M00"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/NewNorm"/>
        <rdfs:label xml:lang="en">conveys</rdfs:label>
        <skos:definition xml:lang="en">A Manifesto conveys a New Norm.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/expresses -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/expresses">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://remogrillo.github.io/maco/isExpressedIn"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/M00"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://remogrillo.github.io/maco/InterpretedConcept"/>
                    <rdf:Description rdf:about="https://remogrillo.github.io/maco/KeyConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">expresses</rdfs:label>
        <skos:definition xml:lang="en">A Manifesto expresses Key Concept or Interpreted Concept. If expresses one, cannot express the other.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/givesShapeTo -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/givesShapeTo">
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/M00"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/ArtisticMovement"/>
        <rdfs:label xml:lang="en">gives shape to</rdfs:label>
        <skos:definition xml:lang="en">What is Futurist as a whole, is determined and somehow regulated by the 22 Futurist Manifestos all together.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/guides -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/guides">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://remogrillo.github.io/maco/InterpretedConcept"/>
                    <rdf:Description rdf:about="https://remogrillo.github.io/maco/KeyConcept"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/ArtisticMovement"/>
        <rdfs:label xml:lang="en">guides</rdfs:label>
        <skos:definition xml:lang="en">Key Concept and Interpreted Concept guides Futurist.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasAbstractionIn -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasAbstractionIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has abstraction in</rdfs:label>
        <skos:definition xml:lang="en">Key Concept has abstraction in an Interpreted Concept. From specification of a Key Concept are extracted common features to define an Interpreted Concept.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasDifferentObjectFrom -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasDifferentObjectFrom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has different object from</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has an Interpreted Concept having a different Object.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasEquivalentConcept -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasEquivalentConcept">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has equivalent concept</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has an equivalent concept in the compared Manifesto, embodied by the Interpreted Concept.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasGeneralizationIn -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasGeneralizationIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has generalization in</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has a generalization in an Interpreted Concept. A Key Concept is made suitable for a general purpose in an Interpreted Concept.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasOpposingEquivalent -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasOpposingEquivalent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has opposing equivalent</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has an equivalent Interpreted Concept expressed in an opposing manner.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasRelatedConcept -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasRelatedConcept">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has related concept</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has a related concept in the compared Manifesto, embodied by the Interpreted Concept.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasRelatedObject -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasRelatedObject">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has related object</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has an Interpreted Concept relating to its same object.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/hasSpecializationIn -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/hasSpecializationIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:range rdf:resource="https://remogrillo.github.io/maco/InterpretedConcept"/>
        <rdfs:label xml:lang="en">has specialization in</rdfs:label>
        <skos:definition xml:lang="en">A Key Concept has a specialization in an Interpreted Concept. A Key Concept is made suitable for a special purpose in an Interpreted Concept.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/inspiresStyleOf -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/inspiresStyleOf">
