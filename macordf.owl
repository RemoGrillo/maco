        <rdfs:subClassOf rdf:resource="https://remogrillo.github.io/maco/KeyConcept"/>
        <rdfs:label xml:lang="en">Dynamism</rdfs:label>
        <skos:definition xml:lang="en">The quality of being characterized by vigorous activity, energy, vitality which results in rapidity of action and decision.</skos:definition>
    </owl:Class>
    


    <!-- https://remogrillo.github.io/maco/InterpretedAttitude -->

    <owl:Class rdf:about="https://remogrillo.github.io/maco/InterpretedAttitude">
