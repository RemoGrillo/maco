                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">is guided by</rdfs:label>
        <skos:definition xml:lang="en">Futurist is guided by Key Concept and Interpreted Concept.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://remogrillo.github.io/maco/isProgrammaticDefinitionOf -->

    <owl:ObjectProperty rdf:about="https://remogrillo.github.io/maco/isProgrammaticDefinitionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
